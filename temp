class Solution:
    # @param n, an integer
    # @return an integer
    def reverseBits(self, n):
        print("{}".format(n))
        #print(type(n))
        multiple=1
        c=0
        while n>0:
            #a1=a%2
            #sum=n%2
            c=(n%10)+10*c
            multiple=multiple*10
            n=n//10
        print(c)
        return c
        